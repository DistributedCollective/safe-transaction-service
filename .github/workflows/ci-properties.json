{
  "app_name": "safe-transaction-service",
  "aws_region": "us-east-2",
  "k8s_cluster_name": "k8-mainnet",
  "registry": "docker.io",
  "image_name": "sovryn/safe-transaction-service",
  "prod_branch": "rsk",
  "dev_branch": "develop",
  "dockerfile_path": "./docker/web",
  "APP_ENV_VARS": {
    "PYTHONPATH": "/app/",
    "DJANGO_SETTINGS_MODULE": "config.settings.local",
    "C_FORCE_ROOT": "true",
